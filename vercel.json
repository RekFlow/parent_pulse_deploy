{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/next"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/python-test",
      "dest": "api/python-test.py"
    },
    {
      "src": "/api/query",
      "dest": "api/query.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
